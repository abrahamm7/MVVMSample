<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage
    x:Class="MVVMSample.Views.MainContentPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:TabbedPages="clr-namespace:MVVMSample.Views;assembly=MVVMSample"
    xmlns:d="http://xamarin.com/schemas/2014/forms/design"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="{Binding Title}"
    mc:Ignorable="d">
    <TabbedPage.Children>
        <TabbedPages:FoodPage Title="Food" />
        <TabbedPages:SearchPage Title="Search" />
        <TabbedPages:LocationPage Title="Location" />
    </TabbedPage.Children>
</TabbedPage>